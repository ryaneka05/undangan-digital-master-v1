generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model WishesInvitationGuest {
  id             Int      @id @default(autoincrement())
  invitationId   Int
  name           String
  message        String
  attending      String
  createDateTime DateTime @default(now())
}
